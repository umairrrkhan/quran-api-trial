{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "chmod +x node_modules/.bin/* && npm run build",
  "outputDirectory": "build"
}